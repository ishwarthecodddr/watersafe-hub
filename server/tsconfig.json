{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "outDir": "dist",
    "rootDir": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "types": ["node"],
    "resolveJsonModule": true
  },
  "include": ["src", "prisma/seed.ts"],
  "exclude": ["dist", "node_modules"]
}